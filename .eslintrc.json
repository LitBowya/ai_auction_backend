{
  "env": {
    "browser": false,
    "es2021": true,
    "node": true
  },
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": ["error", { "singleQuote": false, "semi": true }],
    "no-unused-vars": "warn",
    "no-console": "off"
  }
}
